{
  "manifest_version": 3,
  "name": "RUB Prices",
  "short_name": "RUB Prices",
  "description": "Автоматически конвертирует цену в карточке товара из евро в рубли",
  "version": "3.02",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
      "storage",
      "tabs"
  ],
  "host_permissions": [
    "*://*.dm.de/*",
    "*://*.zara.com/*",
    "*://*.ikea.com/*",
    "*://*.parfumdreams.de/*",
    "*://*.parfumsclub.de/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.dm.de/*",
        "*://*.zara.com/de/*",
        "*://*.ikea.com/de/*",
        "*://*.parfumdreams.de/*",
        "*://*.parfumsclub.de/*"
      ],
      "css": ["styles.css"],
      "js": [
        "js/plugin-manager.js",
        "js/config.js",
        "plugins/dm.js",
        "plugins/zara.js",
        "plugins/ikea.js",
        "plugins/parfumdreams.js",
        "plugins/parfumsclub.js",
        "js/init-plugins.js",
        "js/content.js"
      ],
      "run_at": "document_end"
    }
  ]
}